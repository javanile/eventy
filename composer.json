{
  "name": "javanile/eventy",
  "description": "The WordPress filter/action system in Laravel",
  "keywords": [
    "vtiger",
    "wordpress",
    "action",
    "events",
    "event",
    "filter",
    "action",
    "filters",
    "actions",
    "hook",
    "hooks"
  ],
  "homepage": "https://github.com/tormjens/eventy",
  "license": "MIT",
  "authors": [
    {
      "name": "Tor Morten Jensen",
      "homepage": "https://tormorten.no"
    }
  ],
  "require": {
    "php": ">=7.0",
    "opis/closure": "^3.5",
    "tightenco/collect": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Javanile\\Eventy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EventyTests\\": "tests/"
    }
  }
}
